{
  "name": "small-business",
  "version": "0.0.1",
  "description": "Angular 13 (Typescript + SASS + Hot Reload)",
  "author": {
    "name": "Binyam Darge",
    "email": "bin.darge@gmail.com"
  },
  "keywords": [
    "angular",
    "angular 13",
    "nodejs",
    "typescript",
    "spectron",
    "eslint",
    "sass"
  ],
  "main": "main.js",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf ./dist",
    "config": "node --loader ts-node/esm set-env.ts",
    "ng": "ng",
    "start": "npm run config && ng serve --open",
    "build": "npm run config && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "test": "npm run config && npm run lint && ng test --configuration=test --browsers=ChromeHeadlessCustom",
    "e2e": "ng e2e",
    "e2e:ci": "ng e2e",
    "ci": "npm run clean && npm run lint && ng test --configuration=test --browsers ChromeHeadlessCustom --code-coverage && npm run build:prod -- --deploy-url /small-business/ --base-href /small-business",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "eslint --color -c eslintrc.json --ext .ts ."
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "~11.1.1",
    "@angular-devkit/architect": "~0.1302.6",
