{
  "name": "small-business",
  "version": "0.0.1",
  "description": "Angular 13 (Typescript + SASS + Hot Reload)",
  "author": {
    "name": "Binyam Darge",
    "email": "bin.darge@gmail.com"
  },
  "keywords": [
    "angular",
    "angular 13",
    "nodejs",
    "typescript",
    "spectron",
    "eslint",
    "sass"
  ],
  "main": "main.js",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf ./dist",
    "config": "node --loader ts-node/esm set-env.ts",
    "ng": "ng",
    "start": "npm run config && ng serve --open",
    "build": "npm run config && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "test": "npm run config && npm run lint && ng test --configuration=test --browsers=ChromeHeadlessCustom",
    "e2e": "ng e2e",
    "e2e:ci": "ng e2e",
    "ci": "npm run clean && npm run lint && ng test --configuration=test --browsers ChromeHeadlessCustom --code-coverage && npm run build:prod -- --deploy-url /small-business/ --base-href /small-business",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "eslint --color -c eslintrc.json --ext .ts ."
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "~11.1.0",
    "@angular-devkit/architect": "~0.1302.6",
    "@angular-devkit/build-angular": "13.2.6",
    "@angular-eslint/builder": "~1.2.0",
    "@angular-eslint/eslint-plugin": "2.0.2",
    "@angular/animations": "~13.2.6",
    "@angular/cdk": "~13.2.6",
    "@angular/cli": "~13.2.6",
    "@angular/common": "~13.2.6",
    "@angular/compiler": "~13.2.6",
    "@angular/compiler-cli": "~13.2.6",
    "@angular/core": "~13.2.6",
    "@angular/flex-layout": "~11.0.0-beta.33",
    "@angular/forms": "~13.2.6",
    "@angular/language-service": "~13.2.6",
    "@angular/material": "~13.0.0",
    "@angular/platform-browser": "~13.2.6",
    "@angular/platform-browser-dynamic": "~13.2.6",
    "@angular/router": "~13.2.6",
    "@fortawesome/angular-fontawesome": "^0.8.2",
    "@fortawesome/fontawesome-free": "~5.15.2",
    "@fortawesome/fontawesome-svg-core": "~1.2.34",
    "@fortawesome/free-brands-svg-icons": "~5.15.2",
    "@fortawesome/free-solid-svg-icons": "~5.15.2",
    "@ngrx/effects": "~12.5.1",
    "@ngrx/store": "~12.5.1",
