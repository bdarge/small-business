    "@popperjs/core": "~2.9.0",
    "@types/jasmine": "~3.6.9",
    "@types/jasminewd2": "~2.0.8",
    "@types/mocha": "~5.2.7",
    "@types/node": "~14.14.31",
    "@types/pdfmake": "0.1.17",
    "@typescript-eslint/eslint-plugin": "~4.20.0",
    "@typescript-eslint/eslint-plugin-tslint": "4.21.0",
    "@typescript-eslint/parser": "~4.20.0",
    "bootstrap": "~5.0.0-beta2",
    "chai": "~4.3.1",
    "chalk": "5.0.0",
    "codelyzer": "^6.0.1",
    "colors": "1.4.0",
    "conventional-changelog-cli": "~2.0.25",
    "core-js": "~3.10.0",
    "dotenv": "12.0.4",
    "eslint": "7.23.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "flex-layout": "^1.0.1",
    "hammerjs": "^2.0.8",
    "husky": "6.0.0",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~6.0.0",
    "jwt-decode": "3.1.2",
    "karma": "6.3.8",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "2.0.3",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "karma-spec-reporter": "^0.0.32",
    "mocha": "^8.3.1",
    "ngx-mat-select-search": "3.3.2",
    "npm-run-all": "^4.1.5",
    "prettier": "2.2.1",
    "protractor": "7.0.0",
    "puppeteer": "13.0.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "10.4.0",
    "typescript": "^4.4.4",
    "wait-on": "3.3.0",
    "webdriver-manager": "12.1.8",
    "zone.js": "~0.11.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "engines": {
    "node": ">=10.9.0"
  },
  "dependencies": {
    "i18next": "^19.9.1",
    "i18next-node-fs-backend": "^2.1.3",
    "karma-chrome-launcher": "3.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "rxjs": "^6.6.6",
    "tslib": "^2.1.0",
    "typeorm": "^0.2.31"
  },
  "browserslist": [
    "chrome 83"
  ]
}